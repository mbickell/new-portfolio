{
  "name": "new-portfolio",
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "sass": "^1.32.8"
  },
  "scripts": {
    "compile": "sass src/stylesheets:src/css",
    "delete": "rm -rf ./dist/*",
    "copy": "cp src/index.html src/css src/assets dist -r",
    "predeploy": "yarn delete && yarn copy",
    "deploy": "yarn predeploy && gh-pages -d dist"
  }
}
